.container{
    display: flex;
    flex-direction: column;
    background-color: #f6f6f6;
    border-radius: 40rpx 40rpx 0 0;
}

.topSearch{
    display: flex;
    flex-direction: row;
    background-color: rgb(226, 224, 222);
    border-radius: 50rpx;
    margin-top: 30rpx;
    border: 3rpx solid #ebebeb;
}

.topSearch image{
    margin: 15rpx 20rpx;
}

.topSearch input{
    margin: 10rpx 10rpx;
}

.wrap{
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: space-between;
    padding: 20rpx 20rpx;
    background-color: white;
    border-radius: 16rpx;
    border: 3rpx solid #ebebeb;
}

navigator{
    width: 100%;
    height: 100%;
}

.product{
    display: flex;
    flex-direction: column;
    width: 45%;
    height: 350rpx;
    margin: 10rpx 10rpx;
    /* align-items: center; */
    color: #607D8B;
    /* padding: 6rpx; */
    background-color: white;
    /* justify-content: center; */
    /* border: 5rpx solid #ebebeb; */
    border-radius: 10rpx;

}

.product image{
    width: 100%;
    border-radius:10rpx 10rpx 0 0;
}

.info{
    width: 100%;
    height: 130rpx;
    border: 2rpx solid #ebebeb;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.pro_name{
    margin-top: 15rpx;
    font-size: 30rpx;
    word-spacing: normal;
    color: rgb(179, 20, 9);
}

.info-price{
    display: flex;
    flex-direction: row;
    margin-top: 25rpx;
    line-height: 30rpx;
}

.cnt{
    font-size: 26rpx;
    margin-left: 60rpx;
}

.price{
    color: #f32222;
    font-size: 34rpx;
}